{
  "name": "auth0-nodejs-regular-webapp-sample",
  "version": "1.0.0",
  "description": "Auth0 + NodeJS Regular WebApp seed",
  "repository": "git://github.com/auth0/node-auth0",
  "author": "Auth0",
  "license": "MIT",
  "scripts": {
    "start": "node ./bin/www",
    "build-task:scss-compile": "node-sass-chokidar --source-map true sass/ -o public/stylesheets",
    "build-task:autoprefixer": "postcss public/stylesheets/*.css --use autoprefixer -d public/stylesheets",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'sass/**/*.scss' -c 'npm run sass:build'",
    "dev": "npm-run-all -p sass:*"
  },
  "dependencies": {
    "async": "^2.6.0",
    "autoprefixer": "^7.1.6",
    "body-parser": "^1.16.1",
    "chokidar-cli": "^1.2.0",
    "connect-ensure-login": "^0.1.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "express-session": "^1.11.3",
    "flag-icon-css": "^2.9.0",
    "jQuery": "^1.7.4",
    "jquery": "^3.2.1",
    "jsdom": "^11.3.0",
    "morgan": "^1.8.1",
    "node-sass": "^4.5.3",
    "node-sass-chokidar": "0.0.3",
    "node-sass-middleware": "^0.11.0",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "passport": "^0.3.2",
    "passport-auth0": "^0.6.0",
    "postcss-cli": "^4.1.1",
    "pug": "~2.0.0-beta11",
    "serve-favicon": "^2.4.0",
    "underscore": "^1.8.3",
    "xmlhttprequest": "^1.8.0"
  }
}
